{
  "name": "second_practice",
  "version": "1.0.0",
  "description": "second practice page",
  "main": "index.html",
  "scripts": {
    "sass:watch": "sass --style=expanded --watch styles:dist",
    "production": "sass --style=compressed styles:dist",
    "prefix": "postcss dist/index.css --use autoprefixer --replace",
    "build:css": "npm run production && npm run prefix",
    "parcel:watch": "parcel serve --dist-dir public",
    "parcel:build": "parcel build --dist-dir build --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dearfriendofyours/second_practice.git"
  },
  "keywords": [],
  "author": "YourFriend",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dearfriendofyours/second_practice/issues"
  },
  "homepage": "https://github.com/dearfriendofyours/second_practice#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "parcel": "^2.16.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.93.2"
  }
}
